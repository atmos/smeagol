PATH="<%= @home %>/Developer/bin:<%= @home %>/Developer/share/npm/bin:<%= @home %>/Developer/Cellar/python/2.7.1/bin:<%= @home %>/Developer/sbin:$PATH"; export PATH
MANPATH="<%= @home %>/Developer/share/man:$MANPATH"; export MANPATH
CFLAGS="-I<%= @home %>/Developer/include"; export CFLAGS
CPPFLAGS="-I<%= @home %>/Developer/include"; export CPPFLAGS
CXXFLAGS="-I<%= @home %>/Developer/include"; export CXXFLAGS
LDFLAGS="-L<%= @home %>/Developer/lib"; export LDFLAGS

export CONFIGURE_ARGS="--with-cflags='$CFLAGS' --with-ldflags='$LDFLAGS'"

if [[ -s <%= @home %>/Developer/.rvm/scripts/rvm ]] ; then
  source <%= @home %>/Developer/.rvm/scripts/rvm
fi

if [[ -d <%= @home %>/Developer/Cellar/python/2.7/bin ]]; then
  export PATH=~/Developer/Cellar/python/2.7/bin:$PATH
fi

if [[ -f <%= @home %>/Developer/.nvm/nvm.sh ]]; then
  source <%= @home %>/Developer/.nvm/nvm.sh
fi

if [[ -f <%= @home %>/.cinderella.profile.custom ]]; then
  source ~/.cinderella.profile.custom
fi
